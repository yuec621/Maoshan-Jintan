<!--pages/Scenic/list.wxml-->
<scroll-view scroll-y='true' class='bg' style='min-height:{{winheight}}px'>
<view class='bg'>
  <view class="first-bg">
              
    <block wx:for='{{list}}'>
      
     <view class='item'>
      <image class='item-img' mode='scaleToFill'  bindtap='foodClick' data-url='https://i.meituan.com/changzhou/all/?cid=2&stid_b=1&cateType=poi'data-id='{{item.Id}}' src="{{item.Pic}}" />
      <view class="block-image">酒店预订</view>
     <view class="image-title">{{item.Name}}</view>
     <view class="image-abs">{{item.Name}}vfvfrvbgrbvrfbvrbvrgfrbvfggfbfdfgbvdcvfdvfdv</view>
    </view>
    
  </block>
</view>

  <view class='loading'>{{loading}}</view>
</view>
</scroll-view>
