<!--pages/Scenic/list.wxml-->
<scroll-view scroll-y='true' class='bg' style='min-height:{{winheight}}px'>
<view class='bg'>
  <view class="first-bg">
              
    <block wx:for='{{list}}'>
      
     <view class='item'>
      <image class='item-img' mode='scaleToFill'  bindtap='foodClick' data-url='https://i.meituan.com/awp/h5/hotel/poi/deal.html?poiId=92336596' data-id='{{item.id}}' src="https://www.chinamaoshan.cn{{item.pic}}" />
      <view class="block-image">酒店预订</view>
     <view class="image-title">{{item.name}}</view>
   
    </view>
    
  </block>

     <block wx:for='{{list1}}'>
      
     <view class='item'>
      <image class='item-img' mode='scaleToFill'  bindtap='foodClick' data-url='https://piao.ctrip.com/ticket/dest/t1836931.html' data-id='{{item.Id}}' src="https://www.chinamaoshan.cn{{item.pic}}"/>
      <view class="block-image">门票预订</view>
     <view class="image-title">{{item.name}}</view>
      </view>
  
  </block> 
</view>

  <view class='loading'>{{loading}}</view>
</view>
</scroll-view>
